	<Color x:Key="DefaultBackgroundColor">#363636</Color>
	<SolidColorBrush x:Key="DefaultBackgroundBrush" Color="#363636"/>
	<Color x:Key="FolderBackgroundColor">#242424</Color>
	<SolidColorBrush x:Key="FolderBackgroundBrush" Color="#242424"/>
	<Color x:Key="HighlightTextColor">#242424</Color>
	<Color x:Key="TextColor">#FFFFFF</Color>
	<SolidColorBrush x:Key="TextBrush" Color="#FFFFFF"/>
	<Color x:Key="FolderEllipseColor">#545454</Color>
	<SolidColorBrush x:Key="FolderEllipseBrush" Color="#545454"/>
	<SolidColorBrush x:Key="SelectedFolderBrush" Color="#773079"/>
	<Color x:Key="SelectedFolderColor">#773079</Color>
	<Color x:Key="SelectedFolderBackgroundColor">#DADADA</Color>
	<SolidColorBrush x:Key="SelectedFolderBackgroundBrush" Color="#DADADA"/>
	<FontFamily x:Key="DefaultFontFamily">Arial</FontFamily>

	<Style TargetType="Menu">
		<Style.Resources>
			<Style TargetType="exc:MenuItemEx">
				<Setter Property="BackgroundPopup" Value="{StaticResource DefaultBackgroundBrush}"/>
				<Setter Property="Foreground" Value="#858585"/>
				<Setter Property="FontFamily" Value="{StaticResource DefaultFontFamily}"/>
				<Setter Property="FontSize" Value="14"/>
			</Style>
		</Style.Resources>
		<Setter Property="Background" Value="{x:Null}"/>
	</Style>
